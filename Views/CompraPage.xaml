<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="TesisAppSINMVVM.Views.CompraPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="CompraPage">
    <VerticalStackLayout>
        <Label
            HorizontalOptions="Center"
            Text="Welcome to COMPRA"
            VerticalOptions="Center" />
        <Button
            x:Name="Button_NuevaCompra"
            Clicked="Button_NuevaCompra_Clicked"
            Text="Nueva compra" />
        <Button
            x:Name="Button_Historial"
            Clicked="Button_Historial_Clicked"
            Text="Historial" />
        <ContentView x:Name="ContentView_CompraPageContenidoDinamico"/>

    </VerticalStackLayout>

</ContentPage>